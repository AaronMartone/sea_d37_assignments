id.toLowerCase()) {\n            case 'bm':\n                return 'Windows 3.1x+ bitmap';\n                break;\n            case 'ba':\n                return 'OS/2 struct bitmap array';\n                break;\n            case 'ci':\n                return 'OS/2 struct color icon';\n                break;\n            case 'cp':\n                return 'OS/2 const color pointer';\n                break;\n            case 'ic':\n                return 'OS/2 struct icon';\n                break;\n            case 'pt':\n                return 'OS/2 pointer';\n                break;\n            default:\n                return 'Unknown bitmap';\n        }\n    }\n\n    // export bitmap transformer.\n    module.exports = function bitmapTransformer(options) {\n        \n        // parse the process.argv.\n        var srcFile = options.srcFile || null;\n        var srcFileEnc = options.srcFileEnc || 'utf8';\n        var bufferReadSize = 24;\n        var transform = options.transform || 'grayscale';\n\n        // ensure the provided source file encoding is valid.\n        if (!Buffer.isEncoding(srcFileEnc)) { throw new Error('Unsupported source file encoding specified:' + srcFileEnc); }\n\n        // read in the bitmap. if no encoding is provided as the second argument, the data is returned as a raw buffer.\n        // NOTE if you provide no encoding specification, returned object is a Buffer, otherwise it's a string.\n        fs.readFile(srcFile, function(err, data) {\n            if (err) { throw err; }\n\n            var output = [];\n\n            console.log('Converting:Buffer to JS Array > Start');\n            for (var i = 0; i < data.length; i++) {\n                output.push(data.toString('utf8', i, i+1));\n            }\n            console.log('Converting:Buffer to JS Array > Done');\n\n            // get bitmap header information\n            var header = output.slice(0, (0 + 2)).join('');\n            var size = output.slice(2, (2 + 4)).join('');\n\n            console.log(output[2], output[3], output[4], output[5]);\n\n\n            console.log('Bitmap Type: ' + getBitmapHeaderType(header));;\n            console.log('Bitmap Size: ' + size + ' byte(s)');\n            //console.log('start', x, 'end');\n            \n            // because JS does not handle binary data directly well, we need to convert the buffer over to JS strings.\n            // when reading/writing to/from the filesystem, it is important to deal with purely binary streams.\n\n            // a buffer is a region of physical memory that is used to temporarily store data while it is being moved\n            // from one place to another. converting between buffers and js objects requires knowing how the buffer was\n            // encoded.\n            \n        });\n\n    };\n\n})();",
